#include "circle.h"

#include <cmath>
#include <cstdio>

Circle::Circle() : c(Point c), r(0) {
}
Circle::Circle(Point init_c, double init_r) : c(init_c), r(init_r) {
}
void Circle::move(double dx, double dy) {
  c.move(dx, dy);
}

double Circle::intersectionArea(const Circle & otherCircle) {
  d = c.distance_

      if (d < A.r + B.r) {
    a = A.r * A.r b = B.r * B.r

                          x = (a - b + d * d) / (2 * d) z = x * x y = sqrt(a - z)

                                  if (d < abs(B.r - A.r)) {
      return PI * min(a, b)
    }
    return a * asin(y / A.r) + b * asin(y / B.r) - y * (x + sqrt(z + b - a))
  }
  return 0
}
